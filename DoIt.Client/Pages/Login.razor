@page "/login"
@inject NavigationManager NavigationManager

<h3>Login</h3>

<div>
    <a class="btn btn-block btn-social btn-google" @onclick="GoogleSignIn">
        <span class="fade fa-google"></span> <b>Sign in with Google</b>
    </a>
</div>

@code {
    void GoogleSignIn()
    {
        NavigationManager.NavigateTo("https://localhost:44318/Api/User/GoogleSignIn", true);
    }
}

